{
    "manifest_version": 3,
    "name": "Google Docs Offline",
    "version": "1.80.1",
    "description": "Edit, create, and view your documents, spreadsheets, and presentations â€” all without internet access.",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}

